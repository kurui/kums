<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<!-- edited with XMLSPY v2004 rel. 2 U (http://www.xmlspy.com) by ccggyy2002 (Delphi) -->

<struts-config>
	<form-beans>
		<form-bean name="loginlogListForm"
			type="com.kurui.kums.monitor.LoginLogListForm">
		</form-bean>
		<form-bean name="operateLogListForm"
			type="com.kurui.kums.monitor.OperateLogListForm">
		</form-bean>
	
	</form-beans>

	<action-mappings>
		<action name="loginlogListForm" path="/monitor/loginloglist"
			parameter="thisAction"
			type="org.springframework.web.struts.DelegatingActionProxy"
			validate="false" scope="request">
			<forward name="listclientloginlog"
				path="/monitor/listClientLoginLog.jsp" />
		</action>

		<action name="operateLogListForm" path="/monitor/operateLoglist"
			parameter="thisAction"
			type="org.springframework.web.struts.DelegatingActionProxy"
			validate="false" scope="request">
			<forward name="listOperateLog"
				path="/monitor/listOperateLog.jsp" />
		</action>

		
	</action-mappings>
</struts-config>
