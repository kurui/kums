<?xml version="1.0" encoding="UTF-8"?>
<!-- [syncWithModel=true] -->
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.kurui.kums.library.No" table="no">


		<id name="id" type="long" column="id">
			<generator class="sequence">
				<param name="sequence">seq_no</param>
			</generator>
		</id>

		<property name="nextNo" column="next_no" type="string"
			not-null="false" insert="true" update="true">
		</property>
		<property name="type" column="type" type="string" not-null="false"
			insert="true" update="true">
		</property>



	</class>

</hibernate-mapping>