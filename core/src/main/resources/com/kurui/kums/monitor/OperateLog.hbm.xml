<?xml version="1.0" encoding="UTF-8"?>
<!-- [syncWithModel=true] -->
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.kurui.kums.monitor.OperateLog" table="operate_log">

		<id name="id" type="long" column="id">
			<generator class="sequence">
				<param name="sequence">seq_operatelog</param>
			</generator>
		</id>
		<property name="orderId" column="order_id" type="java.lang.Long"
			not-null="false" insert="true" update="true">
		</property>

		<property name="orderType" column="order_type" type="java.lang.Long"
			not-null="false" insert="true" update="true">
		</property>

		<property name="optTime" column="opt_time" type="java.sql.Timestamp"
			not-null="false" insert="true" update="true">
		</property>

		<property name="type" column="type" type="java.lang.Long"
			not-null="false" insert="true" update="true">
		</property>

		<property name="status" column="status" type="java.lang.Long"
			not-null="false" insert="true" update="true">
		</property>

		<many-to-one name="sysUser" class="com.kurui.kums.right.SysUser"
			not-null="false">
			<column name="user_id" />
		</many-to-one>
		
		<many-to-one name="operateLogDetail"
			class="com.kurui.kums.monitor.OperateLogDetail">
			<column name="OPERATE_LOG_DETAIL_ID" precision="22" scale="0" />
		</many-to-one>
	</class>
</hibernate-mapping>                                                                                                                                                                              