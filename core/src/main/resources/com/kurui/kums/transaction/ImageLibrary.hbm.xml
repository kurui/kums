<?xml version="1.0" encoding="UTF-8"?>
<!-- [syncWithModel=true] -->
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.kurui.kums.transaction.ImageLibrary" table="ImageLibrary">
		<id name="id" type="long" column="id">
			<generator class="sequence">
				<param name="sequence">seq_ImageLibrary</param>
			</generator>
		</id>
		<property name="name" column="name" type="string" not-null="false"
			insert="true" update="true">
		</property>

		<property name="content" column="content"
			type="org.springframework.orm.hibernate3.support.BlobByteArrayType"
			not-null="false" insert="true" update="true">
		</property>

		<property name="type" column="type" type="java.lang.Long"
			not-null="false" insert="true" update="true">
		</property>
		<property name="status" column="status" type="java.lang.Long"
			not-null="false" insert="true" update="true">
		</property>
		<property name="updateTime" column="update_time" type="java.sql.Timestamp"
			not-null="false" insert="true" update="true">
		</property>
		<property name="userNo" column="user_no" type="string"
			not-null="false" insert="true" update="true">
		</property>
		<property name="memo" column="memo" type="string" not-null="false"
			insert="true" update="true">
		</property>

	</class>
</hibernate-mapping>